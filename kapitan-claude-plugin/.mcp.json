{
  "context7": {
    "command": "npx",
    "args": ["-y", "@upstash/context7-mcp@latest"]
  },
  "git": {
    "command": "uvx",
    "args": ["mcp-server-git"]
  },
  "chrome-devtools": {
    "command": "npx",
    "args": ["-y", "chrome-devtools-mcp@latest"]
  },
  "exa": {
    "command": "npx",
    "args": ["-y", "exa-mcp-server", "exaApiKey=${EXA_API_KEY}"]
  }
}
